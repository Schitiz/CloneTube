extends layouts/base
include mixins/videoBlock

block content
    .search_header
        h3 Searching for #{searchingBy}
    .search_videos
        if videos.length===0 
            h5 No videos found
        each video in videos
            +videoBlock({
                title: video.title,
                views: video.views,
                videoFile: video.videoFile,
                id: video.id
            })